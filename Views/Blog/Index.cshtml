@model IList<Article>
@{
    ViewData["Title"] = "Manage Blogs";
}
<h1>Manage Blogs</h1>
<table class="table table-dark">
    <thead>
        <tr>
            <td>Id</td>
            <td>Title</td>
            <td>Author</td>
            <td>Created</td>
            <td>Content</td>
            <td>options</td>
        </tr>
    </thead>
    <tbody>
       @foreach(var article in Model)
            {
                <partial name="_ArticlePartial" model="article" />
            }
    </tbody>
</table>